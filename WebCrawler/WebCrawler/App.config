<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <add key="urlToCrawl" value="http://facebook.com"/>
    <add key="maximumNoOfThreads" value="10"/>
    <add key="maximumNoOfLinksToBeCrawled" value="100"/>
   <add key="linksToBeIgnored" value="javascript:;"/>
    <add key="errorLogPath" value="ErrorLog\ErrorLog.txt"/>
    <add key="VisitedLinkLog" value="VisitedLinkLog"/>
  </appSettings>
</configuration>